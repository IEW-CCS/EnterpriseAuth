<UserControl x:Class="EnterpriseAuth.Views.ConnectView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EnterpriseAuth.Views"
             mc:Ignorable="d" 
             d:DesignHeight="560" d:DesignWidth="600">
    <Grid>
        <Label Content="Authentication" HorizontalAlignment="Center" Margin="0,28,0,0" VerticalAlignment="Top" FontSize="36" FontWeight="Bold"/>
        <Label x:Name="label" Content="{Binding ProfileObject.strProfileName}" HorizontalAlignment="Center" Margin="0,89,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold"/>

    </Grid>
</UserControl>
